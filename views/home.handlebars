{{> nav}}

{{#if articles.length}}
<div class ="container">
<div class="articles-list">
  {{#each articles}}
    <h2><a class="articles-link" href="{{this.link}}">{{this.title}}</a></h2>
    <button type="button" class="btn btn-secondary btn-lg">Save Article</button>
  {{/each}}
</div>
</div>
{{/if}}



